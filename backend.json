          },
          "startDate": {
            "type": "string",
            "example": "2024-03-15T10:30:00.000Z"
          },
          "endDate": {
            "type": "string",
            "example": "2024-03-15T10:30:00.000Z"
          },
          "startHour": {
            "type": "string",
            "example": "09:00"
          },
          "endHour": {
            "type": "string",
            "example": "17:00"
          },
          "durationMinutes": {
            "type": "integer",
            "minimum": 1,
            "example": 30
          },
          "daysOfWeek": {
            "description": "Days of week (1=Monday, 7=Sunday)",
            "example": [
              1,
