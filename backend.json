              4,
              5
            ],
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        }
      },
      "CreateAppointmentDto": {
        "type": "object",
        "properties": {
          "availabilityId": {
            "type": "string",
            "example": "3bc62326-f9ec-4a95-9ae1-51f8ba3d03bf",
            "description": "ID of the availability slot to book"
          },
          "patientId": {
            "type": "string",
            "example": "3bc62326-f9ec-4a95-9ae1-51f8ba3d03bf",
            "description": "Patient ID (only for ADMIN/DOCTOR roles, PATIENT uses token)"
          }
        },
        "required": [
          "availabilityId"
        ]
      }
    }
  }
