          "durationMinutes": {
            "type": "integer",
            "minimum": 1,
            "example": 30
          },
          "daysOfWeek": {
            "description": "Days of week (1=Monday, 7=Sunday)",
            "example": [
              1,
              2,
              3,
              4,
              5
            ],
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        },
        "required": [
          "doctorId",
          "specialtyId",
          "mode",
          "startDate",
          "startHour",
          "endHour",
          "durationMinutes",
          "daysOfWeek"
        ]
      },
      "UpdateConfigAvailabilityDto": {
        "type": "object",
